{
  "all": true,
  "check-coverage": true,
  "extension": [
    ".ts"
  ],
  "include": [
    "src/**/*.ts",
    "src/core/**/*.ts",
    "src/bots/**/*.ts"
  ],
  "exclude": [
    "src/core/server.ts",
    "src/**/index.ts",
    "src/**/types.ts"
  ],
  "loader": "ts-node/esm",
  "branches": 70,
  "lines": 70,
  "functions": 70,
  "statements": 70
}
